{
  "name": "3d-tiles-renderer",
  "version": "0.3.17",
  "type": "module",
  "sideEffects": false,
  "main": "src/index.js",
  "module": "src/index.js",
  "files": [
    "src/*"
  ],
  "scripts": {
    "start": "concurrently \"parcel watch src/*.html --out-dir ./src/dev-bundle/ --public-url . --no-cache\" \"static-server\"",
    "dev": "parcel src/index.html",
    "build": "parcel build index.html --no-content-hash src/*.html --out-dir ./src/bundle/ --public-url . --no-cache",
    "lint": "eslint \"src/**/*.js\" \"test/**/*.js\" \"src/*.js\" && tsc -p tsconfig.json --noEmit",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.8",
    "@types/three": ">=0.128.0",
    "3d-tiles-renderer": "^0.3.17",
    "babel-jest": "^25.5.1",
    "cesium": "^1.97.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-config-mdcs": "^4.2.3",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^25.5.4",
    "jest-cli": "^25.5.4",
    "parcel-bundler": "^1.12.4",
    "static-server": "^2.2.1",
    "three": "^0.152.2",
    "typescript": "^3.9.9"
  },
  "peerDependencies": {
    "three": ">=0.123.0"
  },
  "dependencies": {
    "three-stdlib": "^2.21.10"
  }
}
